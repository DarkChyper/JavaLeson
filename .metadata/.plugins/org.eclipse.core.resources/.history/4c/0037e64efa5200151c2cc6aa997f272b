package fr.class_options;

/**
 * @author dark
 * 
 */
public class SiegeChauffant implements Option {

	/*
	 * (non-Javadoc)
	 * 
	 * @see fr.class_options.Option#getPrix()
	 */
	@Override
	public Double getPrix() {
		return 562.9;
	}

	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "Siege Chauffant";
	}
}
